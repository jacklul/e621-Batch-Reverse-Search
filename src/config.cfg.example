; Debug mode
DEBUG=false

; Path to images directory (should be an absolute path)
PATH_IMAGES="images/"
; Log console output to a file
LOGGING=false

; Path to logs directory
PATH_LOGS="logs/"

; Output HTML
OUTPUT_HTML=true

; HTML file to output to (should be an absolute path)
OUTPUT_HTML_FILE="images/found/links.html"

; Enable searching by MD5
MD5_SEARCH=true

; Enable searching with reverse search (iqdb.harry.lu)
REVERSE_SEARCH=true

; Use php-wfio extension
USE_PHPWFIO=true

; Convert PNG into JPG
CONVERT_PNG=true
